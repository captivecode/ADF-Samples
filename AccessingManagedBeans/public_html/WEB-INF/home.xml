<?xml version="1.0" encoding="windows-1252" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <task-flow-definition id="home">
    <default-activity>__home1</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <managed-bean id="__1">
      <managed-bean-name>RequestBean</managed-bean-name>
      <managed-bean-class>cc.bean.RequestBean</managed-bean-class>
      <managed-bean-scope>request</managed-bean-scope>
    </managed-bean>
    <managed-bean id="__2">
      <managed-bean-name>BackingBean</managed-bean-name>
      <managed-bean-class>cc.bean.BackingBean</managed-bean-class>
      <managed-bean-scope>backingBean</managed-bean-scope>
    </managed-bean>
    <view id="__home1">
      <page>/fragments/home.jsff</page>
    </view>
    <use-page-fragments/>
  </task-flow-definition>
</adfc-config>
